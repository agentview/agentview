  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "tsup": "^8.5.1",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.2",
    "concurrently": "^9.2.1",
    "vitest": "^4.0.7"
