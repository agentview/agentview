{
  "name": "agentview",
  "version": "0.0.4-beta.15",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --clean"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "bin": {
    "agentview": "./dist/bin/agentview.js"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./*": {
        "import": "./dist/*.js",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "dependencies": {
    "better-auth": "^1.2.12",
    "chokidar-cli": "^3.0.0",
    "react": "^19.1.0",
    "react-router": "^7.5.3",
    "zod": "^4.1.11",
    "zod-from-json-schema": "workspace:*",
    "dotenv": "^17.1.0"
  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "tsup": "^8.5.1",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.2",
    "concurrently": "^9.2.1",
    "vitest": "^4.0.7"
  }
}
