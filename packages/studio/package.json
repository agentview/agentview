{
  "name": "@agentview/studio",
  "version": "0.0.4-beta.14",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -Rf dist",
    "watch:tsc": "tsc -w --preserveWatchOutput",
    "build:tsc": "tsc",
    "watch:alias": "tsc-alias -w",
    "build:alias": "tsc-alias",
    "build:tailwindcss-typography": "mkdir -p dist/tailwindcss-typography/src && cp -R src/tailwindcss-typography/src/* dist/tailwindcss-typography/src/",
    "build:styles": "cp src/styles.css dist/styles.css",
    "watch:styles": "npx chokidar 'src/styles.css' -c \"npm run build:styles\"",
    "build": "npx tsup && npm run build:alias && npm run build:tailwindcss-typography && npm run build:styles",
    "dev": "concurrently -n tsc,alias,styles -c blue,magenta,cyan \"npm run watch:tsc\" \"npm run watch:alias\" \"npm run watch:styles\"",
    "dev:studio": "vite dev"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./styles.css": {
      "default": "./dist/styles.css"
    }
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tiptap/extension-document": "^3.0.7",
    "@tiptap/extension-mention": "^3.0.7",
    "@tiptap/extension-paragraph": "^3.0.7",
    "@tiptap/extension-text": "^3.0.7",
    "@tiptap/extensions": "^3.0.7",
    "@tiptap/pm": "^3.0.7",
    "@tiptap/react": "^3.0.7",
    "@tiptap/suggestion": "^3.0.7",
    "better-auth": "^1.2.12",
    "chokidar": "^5.0.0",
    "chokidar-cli": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.1.0",
    "isbot": "^5.1.27",
    "linkify-react": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "lucide-react": "^0.525.0",
    "marked": "^16.3.0",
    "next-themes": "^0.4.6",
    "postcss-selector-parser": "6.0.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.63.0",
    "react-router": "^7.5.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.5",
    "vaul": "^1.1.2",
    "zod": "^4.1.11",
    "zod-from-json-schema": "workspace:*",
    "agentview": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "tsx": "^4.20.3",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.4",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.7"
  }
}
